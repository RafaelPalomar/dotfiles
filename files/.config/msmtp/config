account rafael.palomar@ntnu.no
host smtp.office365.com
port 587
from rafael.palomar@ntnu.no
user rafaelp@ntnu.no
passwordeval "mutt_oauth2.py ~/.password-store/email/ntnu.no"
auth xoauth2
tls on
tls_starttls on
tls_force_sslv3 on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/rafael/.config/msmtp/msmtp.log

